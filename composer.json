{
    "name": "freis/laravel-saml2idp",
    "description": "Make your Laravel application an Identification Provider using SAML 2.0.",
    "keywords": ["laravel", "saml", "saml 2.0", "auth", "acl", "sso", "idp"],
    "license": "MIT",
    "require": {
        "php": "^7.3 | ^8.0 | ^8.1",
        "illuminate/support": "^7.0 | ^8.0 | ^9.0",
        "illuminate/routing": "^7.0 | ^8.0 | ^9.0",
        "litesaml/lightsaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PDMFC\\Saml2Idp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PDMFC\\Saml2Idp\\SamlIdpServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
